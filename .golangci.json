{
    "linters": {
        "disable-all": true,
        "enable": [
            "ineffassign",
            "goimports",
            "govet",
            "misspell",
            "revive",
            "unused"
        ]
    },
    "linters-settings": {
        "govet": {
            "check-shadowing": false
        },
        "revive": {
            "rules": {
                "name": "confusing-naming",
                "disabled": true
            }
        }
    },
    "run": {
        "skip-dirs": [
            "pkg/generated"
        ],
        "tests": false,
        "timeout": "10m"
    }
}